window.onload = function () {
    console.log("This file is connected");
    var glow = true;

    function moveRightTank() {
        $(".w-tank").css({ "right": "-=50px" });  // Move the image 50 pixels to the left
    }

    function moveRightPounder() {
        $(".six-pounder").css({ "right": "-=50px" });  // Move the image 50 pixels to the left
    }

    $(".w-tank").hover(moveRightTank);
    $(".six-pounder").hover(moveRightPounder);
}
